<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="picturePath" class="java.lang.String"
		c:value="${savePath.member.picture}" />
	<bean id="summernotePath" class="java.lang.String"
		c:value="${savePath.summernote.image}" />
	<bean id="logSavedPath" class="java.lang.String"
		c:value="${savePath.log.files}" />
	<bean id="memberDAO" class="com.Semicolon.cmnt.dao.MemberDAOImpl"
		c:session-ref="sqlSession" />
	<bean id="boardDAO" class="com.Semicolon.cmnt.dao.BoardDAOImpl"
		c:session-ref="sqlSession" />
	<bean id="replyDAO" class="com.Semicolon.cmnt.dao.ReplyDAOImpl"
		c:session-ref="sqlSession" />
	<bean id="engineerDAO"
		class="com.Semicolon.cmnt.dao.EngineerDAOImpl"
		c:session-ref="sqlSession" />
	<bean id="recruit_boardDAO"
		class="com.Semicolon.cmnt.dao.Recruit_BoardDAOImpl"
		c:session-ref="sqlSession" />
	<bean id="recruit_boardService"
		class="com.Semicolon.cmnt.service.Recruit_BoardServiceImpl"
		c:recruit_boardDAO-ref="recruit_boardDAO"
		c:summernotePath="some/path/here" c:replyDAO-ref="replyDAO" />
	<bean id="engineerService"
		class="com.Semicolon.cmnt.service.EngineerServiceImpl"
		c:engineerDAO-ref="engineerDAO" />
	<bean id="replyService"
		class="com.Semicolon.cmnt.service.ReplyServiceImpl"
		c:replyDAO-ref="replyDAO" />
	<bean id="boardService"
		class="com.Semicolon.cmnt.service.BoardServiceImpl"
		c:boardDAO-ref="boardDAO" c:summernotePath-ref="summernotePath"
		c:replyDAO-ref="replyDAO" />
	<bean id="memberService"
		class="com.Semicolon.cmnt.service.MemberServiceImpl"
		c:memberDAO-ref="memberDAO" />
	<bean id="recruit_applicationDAO"
		class="com.Semicolon.cmnt.dao.Recruit_ApplicationDAOImpl">
		<constructor-arg ref="sqlSession" />
	</bean>
	<bean id="recruit_ApplicationService"
		class="com.Semicolon.cmnt.service.Recruit_ApplicationService">
		<constructor-arg ref="recruit_applicationDAO" />
	</bean>



	<bean id="createOrgDAO"
		class="com.Semicolon.org.dao.CreateOrgDAOImpl"
		c:sqlSession-ref="sqlSession" />

	<bean id="createOrgService"
		class="com.Semicolon.org.service.CreateOrgServiceImpl"
		c:createOrgDAO-ref="createOrgDAO" />
	<bean id="orgDetailDAO"
		class="com.Semicolon.org.dao.OrgDetailDAOImpl"
		c:sqlSession-ref="sqlSession" />
	<bean id="orgDetailService"
		class="com.Semicolon.org.service.OrgDetailServiceImpl"
		c:orgDetailDAO-ref="orgDetailDAO" />

	<bean id="projectOrgDAO"
		class="com.Semicolon.org.dao.ProjectOrgDAOImpl"
		c:sqlSession-ref="sqlSession" />

	<bean id="projectOrgService"
		class="com.Semicolon.org.service.ProjectOrgServiceImpl"
		c:projectOrgDAO-ref="projectOrgDAO" />

	<bean id="taskDAO" class="com.Semicolon.pms.dao.TaskDAOImpl"
		c:sqlSession-ref="sqlSession" />
	<bean id="taskService"
		class="com.Semicolon.pms.service.TaskServiceImpl"
		c:taskDAO-ref="taskDAO" c:taskReplyDAO-ref="taskReplyDAO" />

	<bean id="issueDAO" class="com.Semicolon.pms.dao.IssueDAOImpl"
		c:sqlSession-ref="sqlSession" />
	<bean id="issueService"
		class="com.Semicolon.pms.service.IssueServiceImpl"
		c:issueDAO-ref="issueDAO" c:issueReplyDAO-ref="issueReplyDAO" />

	<bean id="ganttDAO" class="com.Semicolon.pms.dao.GanttDAOImpl"
		c:sqlSession-ref="sqlSession" />
	<bean id="ganttService"
		class="com.Semicolon.pms.service.GanttServiceImpl"
		c:ganttDAO-ref="ganttDAO" />

	<bean id="calendarDAO"
		class="com.Semicolon.pms.dao.CalendarDAOImpl"
		c:sqlSession-ref="sqlSession" />
	<bean id="calendarService"
		class="com.Semicolon.pms.service.CalendarServiceImpl"
		c:calendarDAO-ref="calendarDAO" />
	<bean id="budgetDAO" class="com.Semicolon.pms.dao.BudgetDAOImpl"
		c:sqlSession-ref="sqlSession" />
	<bean id="budgetService"
		class="com.Semicolon.pms.service.BudgetServiceImpl"
		c:budgetDAO-ref="budgetDAO" />
	<bean id="member1DAO" class="com.Semicolon.org.dao.Member1DAOImpl"
		c:sqlSession-ref="sqlSession" />

	<bean id="member1Service"
		class="com.Semicolon.org.service.Member1ServiceImpl">
		<constructor-arg ref="member1DAO" />
	</bean>


	<bean id="taskReplyDAO"
		class="com.Semicolon.pms.dao.TaskReplyDAOImpl">
		<property name="sqlSession" ref="sqlSession" />
	</bean>

	<bean id="taskReplyService"
		class="com.Semicolon.pms.service.TaskReplyServiceImpl">
		<property name="taskReplyDAO" ref="taskReplyDAO" />
	</bean>

	<bean id="issueReplyDAO"
		class="com.Semicolon.pms.dao.IssueReplyDAOImpl">
		<constructor-arg ref="sqlSession" />
	</bean>

	<bean id="issueReplyService"
		class="com.Semicolon.pms.service.IssueReplyServiceImpl">
		<constructor-arg ref="issueReplyDAO" />
	</bean>


	<bean id="fundingSavedFilePath" class="java.lang.String"
		c:value="${savePath.funding.files}" />

	<bean id="reportSavedFilePath" class="java.lang.String"
		c:value="${savePath.report.files}" />

	<bean id="projectSavedFilePath" class="java.lang.String"
		c:value="${savePath.project.files}" />

	<bean id="adminreportSavedFilePath" class="java.lang.String"
		c:value="${savePath.adminreport.files}" />

	<bean id="fundingDAO" class="com.Semicolon.dao.FundingDAOImpl"
		c:session-ref="sqlSession" />
	<bean id="attachDAO" class="com.Semicolon.dao.AttachDAOImpl"
		c:session-ref="sqlSession" />

	<bean id="attachreportDAO"
		class="com.Semicolon.dao.AttachReportDAOImpl"
		c:session-ref="sqlSession" />

	<bean id="reportDAO" class="com.Semicolon.dao.ReportDAOImpl"
		c:session-ref="sqlSession" />

	<bean id="heartDAO" class="com.Semicolon.dao.HeartDAOImpl"
		c:session-ref="sqlSession" />

	<bean id="meetingDAO" class="com.Semicolon.dao.MeetingDAOImpl"
		c:session-ref="sqlSession" />

	<bean id="adminreportDAO"
		class="com.Semicolon.dao.AdminReportDAOImpl"
		c:session-ref="sqlSession" />


	<bean id="meetingService"
		class="com.Semicolon.service.MeetingServiceImpl"
		c:meetingDAO-ref="meetingDAO" />


	<bean id="adminreportService"
		class="com.Semicolon.service.AdminReportServiceImpl"
		c:adminreportDAO-ref="adminreportDAO">
	</bean>


	<bean id="fundingService"
		class="com.Semicolon.service.FundingServiceImpl"
		c:fundingDAO-ref="fundingDAO" c:attachDAO-ref="attachDAO"
		c:heartDAO-ref="heartDAO">
		<!-- HeartDAO는 property로 주입 -->
	</bean>

	<bean id="reportService"
		class="com.Semicolon.service.ReportServiceImpl"
		c:reportDAO-ref="reportDAO" c:attachreportDAO-ref="attachreportDAO" />

	<bean id="menuDAO" class="com.Semicolon.dao.MenuDAOImpl"
		c:session-ref="sqlSession" />

	<bean id="menuService"
		class="com.Semicolon.service.MenuServiceImpl" c:menuDAO-ref="menuDAO" />
</beans>